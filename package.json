{
  "name": "pixlee-code-challenge",
  "version": "0.2.0",
  "description": "Code challenge for Pixlee",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:frontend && npm run serve",
    "serve": "npm run build:backend && node ./bin/server/server.js",
    "build:backend": "./node_modules/.bin/babel src/backend -d bin/server",
    "build:frontend": "node node_modules/.bin/webpack",
    "webpackdev": "npm run build:frontend && ./node_modules/.bin/webpack-dev-server --output-public-path=/bin/app/ --progress --colors",
    "test": "mocha --compilers js:babel-core/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianboyko/pixlee-code-challenge.git"
  },
  "keywords": [
    "Photos",
    "pixlee",
    "code-challenge"
  ],
  "author": "Brian Boyko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brianboyko/pixlee-code-challenge/issues"
  },
  "homepage": "https://github.com/brianboyko/pixlee-code-challenge#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.0.0",
